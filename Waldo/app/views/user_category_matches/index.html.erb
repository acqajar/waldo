<p id="notice"><%= notice %></p>

<h1>Listing User Category Matches</h1>

<table>
  <thead>
    <tr>
      <th>Match</th>
      <th>User category</th>
      <th>Uc</th>
      <th>Match</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_category_matches.each do |user_category_match| %>
      <tr>
        <td><%= user_category_match.match %></td>
        <td><%= user_category_match.user_category %></td>
        <td><%= user_category_match.uc_id %></td>
        <td><%= user_category_match.match_id %></td>
        <td><%= link_to 'Show', user_category_match %></td>
        <td><%= link_to 'Edit', edit_user_category_match_path(user_category_match) %></td>
        <td><%= link_to 'Destroy', user_category_match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User category match', new_user_category_match_path %>
